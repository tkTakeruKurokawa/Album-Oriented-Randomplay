    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "msw-storybook-addon": "^2.0.5",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "storybook": "8.6.14",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^15.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
